#include<iostream>
using namespace std;

int main() {
    int matrix[3][3];
    
    cout << " Please enter the 3x3 elemets of the matrix";
    for (int i = 0;i<3;i++){
        for (int j = 0; j<3 ; j++){
        cout << "element of [" << i << "]" << " And [" << j << "]" << endl ;
        cin >> matrix[i][j];
    }
}

cout << "Here is the 3x3 matrix : " << endl ;
for ( int i = 0; i<3; i++){
    for(int j = 0 ; j<3 ; j++){
        cout << matrix[i][j] << " ";
    }
    cout << endl;
}

return 0;
}
